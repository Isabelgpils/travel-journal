<main class="container container-page">
  <h1 class= "pt-5 pb-5 mt-3">Current Sign Ups</h1>
  <% @signups.each do |signup| %>
    <section class="container pt-5 pb-5">
      <h3><%= signup.firstname %></h3>
      <h3 class="text-warning"><%= signup.email %></h3>
      <h3><%= signup.created_at.strftime("%m/%d/%Y") %></h3>
    </section>
  <% end %>
</main>
